				<select name="attr_npc_output_option">
                    <option value="!bw " selected="selected">Blind Whispered (Requires API Script)</option>
					<option value="/w GM " selected="selected">Whispered to the GM</option>
					<option value=" ">Public for all to see</option>
				</select>
			</div>
		</div>

